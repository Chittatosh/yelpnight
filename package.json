{
  "name": "yelpnight",
  "version": "1.0.0",
  "description": "freeCodeCamp Nightlife Coordination App",
  "main": "src/index.js",
  "repository": "https://github.com/Chittatosh/yelpnight",
  "author": "Chittatosh Undirwade <chittatosh.u@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node src/index.js",
    "buildprod": "webpack",
    "serverprod": "set NODE_ENV=production && node -r dotenv/config src",
    "serverdev": "nodemon -e js,graphql --watch src -r dotenv/config",
    "clientdev": "webpack-dev-server --config ./client.webpack.config.js",
    "fullstackdev": "npm-run-all --parallel serverdev clientdev",
    "playground": "graphql playground",
    "graphqldev": "npm-run-all --parallel serverdev playground",
    "wpstats": "webpack --json > logs/wpstats.json"
  },
  "dependencies": {
    "apollo-boost": "^0.1.4",
    "bcryptjs": "2.4.3",
    "express": "^4.16.3",
    "graphql-yoga": "1.8.5",
    "jsonwebtoken": "8.2.1",
    "pretty-error": "^2.1.1",
    "prisma-binding": "1.5.17",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-apollo": "^2.1.3",
    "react-dom": "^16.3.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "yelp-fusion": "^2.0.3"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "dotenv": "5.0.1",
    "graphql-cli": "2.15.9",
    "nodemon": "1.17.3",
    "npm-run-all": "4.1.2",
    "prisma": "1.6.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  }
}
